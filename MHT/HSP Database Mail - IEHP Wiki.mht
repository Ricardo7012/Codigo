      <P>MO=3DSQLAGENTMOSVC </P>
      <P>PROD=3DSQLHSP1AGENTSVC </P></TD>
    <TD>USE [msdb]=20
      <P>GO </P>
      <P>CREATE USER [IEHP\sqlprocessadmin] FOR LOGIN =
[IEHP\sqlprocessadmin]=20
</P>
      <P>GO </P>
      <P>USE [msdb] </P>
      <P>GO </P>
      <P>ALTER ROLE [DatabaseMailUserRole] ADD MEMBER =
[IEHP\sqlprocessadmin]=20
</P>
      <P>GO <BR></P></TD></TR>
